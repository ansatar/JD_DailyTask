{
  //数据库连接字符串
  "ConnectionStrings": {
    "Default": "server=localhost; port=3306; database=jd_dailytask; uid=root; password=123456;"
  },
  //默认的
  "Logging": {
    "LogLevel": {
      "Default": "Trace", //Trace->Debug->Information->Warning->Error->Fatal
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  //Serilog
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug", //日志等级有小到大：Verbose->Debug->Information->Warning->Error->Fatal
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/serilog/log-.txt",
          "rollingInterval": "Day",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
        }
      }
    ]
  },
  //自动任务清单
  "AutoTaskList": [
    //"DoSignShop"

    //京豆
    "DoSignBean",
    "DoSignSupermarket",
    "DoSignWebcasts",
    "DoSignLotteryDraw",
    "DoSignShake",
    "DoSignFlashSale",
    "DoSignLuckyLottery",
    "DoSignJingRongDoll",
    "DoSignShop",
    "DoGameBySpeedUp",
    //钢G
    "DoSignRongSteel",
    //金贴
    "DoOpenScreenReward",
    "DoSignSubsidy",
    //现金红包
    "DoSignCash",
    "DoSignGetCash",
    "DoSignSecKilling"
  ],
  //京东用户过期时间（天）
  "JdUserExpiresDay": 30
}
